{
  "name": "opa.geburtstag.hundertmark.org",
  "version": "1.0.0",
  "description": "Geschenk zu Opa Andreas' 65. Geburtstag von Noah",
  "main": "main.js",
  "dependencies": {
    "electron-packager": "^12.1.1",
    "load-json-file": "^5.0.0"
  },
  "devDependencies": {
    "electron": "2.0.8"
  },
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/gift-icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . birthday --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/gift-icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Opas Geburtstag\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/gift-icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1337-5P34Ker/birthday.git"
  },
  "keywords": [
    "Opa",
    "Andreas"
  ],
  "author": "Noah + Rocco Hundertmark",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/1337-5P34Ker/birthday/issues"
  },
  "homepage": "https://github.com/1337-5P34Ker/birthday#readme"
}
